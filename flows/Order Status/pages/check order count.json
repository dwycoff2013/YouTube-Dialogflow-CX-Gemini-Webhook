{
  "name": "27f93652-d277-48ea-82f8-d317e6ea3a94",
  "displayName": "check order count",
  "form": {
  
  },
  "entryFulfillment": {
    "setParameterActions": [ {
      "parameter": "$flow.order_id",
      "value": "$session.params.all_recent_order_id[0]"
    } ]
  },
  "transitionRoutes": [ {
    "condition": "$sys.func.COUNT($session.params.all_recent_order_id) \u003e= 1",
    "triggerFulfillment": {
      "setParameterActions": [ {
        "parameter": "$flow.check_recent_order_details",
        "value": "true"
      } ]
    },
    "targetPage": "\u003e get order details API",
    "name": "96941a09-9f69-46a5-ba39-5f5ee2338a5d"
  }, {
    "condition": "$sys.func.COUNT($session.params.all_recent_order_id) \u003c 1 AND $flow.retry_alternate_profile_counter \u003c $flow.max_retry_alternate_profile",
    "triggerFulfillment": {
      "setParameterActions": [ {
        "parameter": "$flow.retry_alternate_profile_counter",
        "value": "$sys.func.ADD($flow.retry_alternate_profile_counter,1)"
      } ]
    },
    "targetPage": "\u003e confirm another account",
    "name": "f2f5ab76-0092-4916-ab0c-28b816f300a2"
  }, {
    "condition": "$flow.retry_alternate_profile_counter = $flow.max_retry_alternate_profile",
    "triggerFulfillment": {
    
    },
    "targetPage": "end failure",
    "name": "72944bdf-5500-4be4-8872-c95d8015ebaa"
  } ]
}
